        {
            "aya_id": 5932,
            "aya_number": 1,
            "aya_text": "وَالسَّمَاۤءِ وَالطَّارِقِۙ ",
            "sura_id": 86,
            "juz_id": 30,
            "page_number": 591,
            "translation_aya_text": "Demi langit dan yang datang pada malam hari.",
            "tafsir_jalalayn": "(Demi langit dan yang datang pada malam hari) lafal Ath-Thaariq pada asalnya berarti segala sesuatu yang datang pada malam hari, antara lain ialah bintang-bintang, karena bintang-bintang baru muncul bila malam tiba.",
            "sajda": false,
            "transliteration": "Wassamaaa'i wattaariq",
            "tajweed_text": "وَ[h:9999[ٱ][l[ل]سَّم[o[َا]ٓءِ وَ[h:14418[ٱ][l[ل]طَّارِ[q[ق]ِ",
